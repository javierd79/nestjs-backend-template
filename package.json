{
  "name": "cda_backend",
  "version": "2.4.3",
  "author": "Dataweb C.A",
  "homepage": "https://www.rifa-max.com/",
  "description": "CDA - Backend admin system with NestJS",
  "private": true,
  "license": "MIT License",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "NODE_ENV=dev nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=prod dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/bull": "^0.6.2",
    "@nestjs/common": "8.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "8.0.0",
    "@nestjs/event-emitter": "^1.3.1",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/mongoose": "9.0.2",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "8.0.0",
    "@nestjs/platform-socket.io": "8.0.0",
    "@nestjs/platform-ws": "8.0.0",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/websockets": "8.0.0",
    "@tinyhttp/app": "^2.0.29",
    "@types/mongoose-auto-increment": "^5.0.35",
    "@types/multer": "^1.4.7",
    "axios": "^1.2.2",
    "bcrypt": "^5.1.0",
    "bull": "^4.10.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "hbs": "^4.2.0",
    "moment": "^2.29.4",
    "mongoose": "6.1.7",
    "mongoose-auto-increment": "^5.0.1",
    "multer": "1.4.5-lts.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "rimraf": "3.0.2",
    "rxjs": "7.2.0",
    "socket.io": "^4.6.0",
    "socket.io-adapter": "^2.5.2",
    "typegoose": "^5.9.1"
  },
  "devDependencies": {
    "@nestjs/cli": "8.0.0",
    "@nestjs/schematics": "8.0.0",
    "@nestjs/testing": "8.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^4.10.0",
    "@types/cron": "^2.0.0",
    "@types/express": "4.17.13",
    "@types/jest": "27.0.2",
    "@types/node": "16.0.0",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.0.0",
    "@typescript-eslint/parser": "5.0.0",
    "eslint": "8.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.2.5",
    "nestjs-admin": "^0.4.1",
    "prettier": "2.3.2",
    "secure-random": "^1.1.2",
    "source-map-support": "0.5.20",
    "supertest": "6.1.3",
    "ts-jest": "27.0.3",
    "ts-loader": "9.2.3",
    "ts-node": "10.0.0",
    "tsconfig-paths": "3.10.1",
    "typescript": "4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
